@article{adePlanck2015Results2016,
	title = {Planck 2015 results - {XIII}. Cosmological parameters},
	volume = {594},
	rights = {© {ESO}, 2016},
	issn = {0004-6361, 1432-0746},
	url = {https://www.aanda.org/articles/aa/abs/2016/10/aa25830-15/aa25830-15.html},
	doi = {10.1051/0004-6361/201525830},
	abstract = {This paper presents cosmological results based on full-mission {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} observations of temperature and polarization anisotropies of the cosmic microwave background ({CMB}) radiation. Our results are in very good agreement with the 2013 analysis of the {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} nominal-mission temperature data, but with increased precision. The temperature and polarization power spectra are consistent with the standard spatially-flat 6-parameter Λ{CDM} cosmology with a power-law spectrum of adiabatic scalar perturbations (denoted “base Λ{CDM}” in this paper). From the {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} temperature data combined with {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} lensing, for this cosmology we find a Hubble constant, {\textless}i{\textgreater}H{\textless}i/{\textgreater}{\textless}sub{\textgreater}0{\textless}sub/{\textgreater} = (67.8 ± 0.9) km s{\textless}sup{\textgreater}-1{\textless}sup/{\textgreater}Mpc{\textless}sup{\textgreater}-1{\textless}sup/{\textgreater}, a matter density parameter Ω{\textless}sub{\textgreater}m{\textless}sub/{\textgreater} = 0.308 ± 0.012, and a tilted scalar spectral index with {\textless}i{\textgreater}n{\textless}i/{\textgreater}{\textless}sub{\textgreater}s{\textless}sub/{\textgreater} = 0.968 ± 0.006, consistent with the 2013 analysis. Note that in this abstract we quote 68\% confidence limits on measured parameters and 95\% upper limits on other parameters. We present the first results of polarization measurements with the Low Frequency Instrument at large angular scales. Combined with the {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} temperature and lensing data, these measurements give a reionization optical depth of {\textless}i{\textgreater}τ{\textless}i/{\textgreater} = 0.066 ± 0.016, corresponding to a reionization redshift of . These results are consistent with those from {WMAP} polarization measurements cleaned for dust emission using 353-{GHz} polarization maps from the High Frequency Instrument. We find no evidence for any departure from base Λ{CDM} in the neutrino sector of the theory; for example, combining {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} observations with other astrophysical data we find {\textless}i{\textgreater}N{\textless}i/{\textgreater}{\textless}sub{\textgreater}eff{\textless}sub/{\textgreater} = 3.15 ± 0.23 for the effective number of relativistic degrees of freedom, consistent with the value {\textless}i{\textgreater}N{\textless}i/{\textgreater}{\textless}sub{\textgreater}eff{\textless}sub/{\textgreater} = 3.046 of the Standard Model of particle physics. The sum of neutrino masses is constrained to ∑ {\textless}i{\textgreater}m{\textless}i/{\textgreater}{\textless}sub{\textgreater}{\textless}i{\textgreater}ν{\textless}i/{\textgreater}{\textless}sub/{\textgreater} {\textless} 0.23 {eV}. The spatial curvature of our Universe is found to be very close to zero, with {\textbar} Ω{\textless}sub{\textgreater}{\textless}i{\textgreater}K{\textless}i/{\textgreater}{\textless}sub/{\textgreater} {\textbar} {\textless} 0.005. Adding a tensor component as a single-parameter extension to base Λ{CDM} we find an upper limit on the tensor-to-scalar ratio of {\textless}i{\textgreater}r{\textless}i/{\textgreater}{\textless}sub{\textgreater}0.002{\textless}sub/{\textgreater}{\textless} 0.11, consistent with the {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} 2013 results and consistent with the {\textless}i{\textgreater}B{\textless}i/{\textgreater}-mode polarization constraints from a joint analysis of {BICEP}2, Keck Array, and {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} ({BKP}) data. Adding the {BKP} {\textless}i{\textgreater}B{\textless}i/{\textgreater}-mode data to our analysis leads to a tighter constraint of {\textless}i{\textgreater}r{\textless}i/{\textgreater}{\textless}sub{\textgreater}0.002{\textless}sub/{\textgreater} {\textless} 0.09 and disfavours inflationarymodels with a {\textless}i{\textgreater}V{\textless}i/{\textgreater}({\textless}i{\textgreater}φ{\textless}i/{\textgreater}) ∝ {\textless}i{\textgreater}φ{\textless}i/{\textgreater}{\textless}sup{\textgreater}2{\textless}sup/{\textgreater} potential. The addition of {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} polarization data leads to strong constraints on deviations from a purely adiabatic spectrum of fluctuations. We find no evidence for any contribution from isocurvature perturbations or from cosmic defects. Combining {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} data with other astrophysical data, including Type Ia supernovae, the equation of state of dark energy is constrained to {\textless}i{\textgreater}w{\textless}i/{\textgreater} = −1.006 ± 0.045, consistent with the expected value for a cosmological constant. The standard big bang nucleosynthesis predictions for the helium and deuterium abundances for the best-fit {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} base Λ{CDM} cosmology are in excellent agreement with observations. We also constraints on annihilating dark matter and on possible deviations from the standard recombination history. In neither case do we find no evidence for new physics. The {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} results for base Λ{CDM} are in good agreement with baryon acoustic oscillation data and with the {JLA} sample of Type Ia supernovae. However, as in the 2013 analysis, the amplitude of the fluctuation spectrum is found to be higher than inferred from some analyses of rich cluster counts and weak gravitational lensing. We show that these tensions cannot easily be resolved with simple modifications of the base Λ{CDM} cosmology. Apart from these tensions, the base Λ{CDM} cosmology provides an excellent description of the {\textless}i{\textgreater}Planck{\textless}i/{\textgreater} {CMB} observations and many other astrophysical data sets.},
	pages = {A13},
	journaltitle = {Astronomy \& Astrophysics},
	shortjournal = {A\&A},
	author = {Ade, P. a. R. and Aghanim, N. and Arnaud, M. and Ashdown, M. and Aumont, J. and Baccigalupi, C. and Banday, A. J. and Barreiro, R. B. and Bartlett, J. G. and Bartolo, N. and Battaner, E. and Battye, R. and Benabed, K. and Benoît, A. and Benoit-Lévy, A. and Bernard, J.-P. and Bersanelli, M. and Bielewicz, P. and Bock, J. J. and Bonaldi, A. and Bonavera, L. and Bond, J. R. and Borrill, J. and Bouchet, F. R. and Boulanger, F. and Bucher, M. and Burigana, C. and Butler, R. C. and Calabrese, E. and Cardoso, J.-F. and Catalano, A. and Challinor, A. and Chamballu, A. and Chary, R.-R. and Chiang, H. C. and Chluba, J. and Christensen, P. R. and Church, S. and Clements, D. L. and Colombi, S. and Colombo, L. P. L. and Combet, C. and Coulais, A. and Crill, B. P. and Curto, A. and Cuttaia, F. and Danese, L. and Davies, R. D. and Davis, R. J. and Bernardis, P. de and Rosa, A. de and Zotti, G. de and Delabrouille, J. and Désert, F.-X. and Valentino, E. Di and Dickinson, C. and Diego, J. M. and Dolag, K. and Dole, H. and Donzelli, S. and Doré, O. and Douspis, M. and Ducout, A. and Dunkley, J. and Dupac, X. and Efstathiou, G. and Elsner, F. and Enßlin, T. A. and Eriksen, H. K. and Farhang, M. and Fergusson, J. and Finelli, F. and Forni, O. and Frailis, M. and Fraisse, A. A. and Franceschi, E. and Frejsel, A. and Galeotta, S. and Galli, S. and Ganga, K. and Gauthier, C. and Gerbino, M. and Ghosh, T. and Giard, M. and Giraud-Héraud, Y. and Giusarma, E. and Gjerløw, E. and González-Nuevo, J. and Górski, K. M. and Gratton, S. and Gregorio, A. and Gruppuso, A. and Gudmundsson, J. E. and Hamann, J. and Hansen, F. K. and Hanson, D. and Harrison, D. L. and Helou, G. and Henrot-Versillé, S. and Hernández-Monteagudo, C. and Herranz, D. and Hildebrandt, S. R. and Hivon, E. and Hobson, M. and Holmes, W. A. and Hornstrup, A. and Hovest, W. and Huang, Z. and Huffenberger, K. M. and Hurier, G. and Jaffe, A. H. and Jaffe, T. R. and Jones, W. C. and Juvela, M. and Keihänen, E. and Keskita