  eprint = {1807.06209},
  eprinttype = {arxiv},
  file = {/home/jacopo/zotero/storage/UN9YH5SZ/Planck Collaboration et al. - 2019 - Planck 2018 results. VI. Cosmological parameters.pdf;/home/jacopo/zotero/storage/8W8SEBAW/1807.html},
