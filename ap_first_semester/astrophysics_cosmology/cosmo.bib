@article{poulinEarlyDarkEnergy2019,
  title = {Early {{Dark Energy Can Resolve The Hubble Tension}}},
  author = {Poulin, Vivian and Smith, Tristan L. and Karwal, Tanvi and Kamionkowski, Marc},
  date = {2019-06-04},
  journaltitle = {Physical Review Letters},
  shortjournal = {Phys. Rev. Lett.},
  volume = {122},
  pages = {221301},
  issn = {0031-9007, 1079-7114},
  doi = {10.1103/PhysRevLett.122.221301},
  url = {http://arxiv.org/abs/1811.04083},
  urldate = {2020-03-05},
  abstract = {Early dark energy (EDE) that behaves like a cosmological constant at early times (redshifts \$z\textbackslash gtrsim3000\$) and then dilutes away like radiation or faster at later times can solve the Hubble tension. In these models, the sound horizon at decoupling is reduced resulting in a larger value of the Hubble parameter \$H\_0\$ inferred from the cosmic microwave background (CMB). We consider two physical models for this EDE, one involving an oscillating scalar field and another a slowly-rolling field. We perform a detailed calculation of the evolution of perturbations in these models. A Markov Chain Monte Carlo search of the parameter space for the EDE parameters, in conjunction with the standard cosmological parameters, identifies regions in which \$H\_0\$ inferred from \{\textbackslash it Planck\} CMB data agrees with the SH0ES local measurement. In these cosmologies, current baryon acoustic oscillation and supernova data are described as successfully as in \textbackslash LCDM, while the fit to \{\textbackslash it Planck\} data is slightly improved. Future CMB and large-scale-structure surveys will further probe this scenario.},
  archivePrefix = {arXiv},
  eprint = {1811.04083},
  eprinttype = {arxiv},
  file = {/home/jacopo/zotero/storage/K27XY7AZ/Poulin et al. - 2019 - Early Dark Energy Can Resolve The Hubble Tension.pdf;/home/jacopo/zotero/storage/9DCV76YR/1811.html},
  keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics,High Energy Physics - Phenomenology},
  note = {Comment: v1: 4p+appendix, 3 figures. v2: small changes, notably estimates of bayesian evidence. matches the version published in PRL},
  number = {22}
}

@article{riessDeterminationLocalValue2016,
  title = {A 2.4\% {{Determination}} of the {{Local Value}} of the {{Hubble Constant}}},
  author = {Riess, Adam G. and Macri, Lucas M. and Hoffmann, Samantha L. and Scolnic, Dan and Casertano, Stefano and Filippenko, Alexei V. and Tucker, Brad E. and Reid, Mark J. and Jones, David O. and Silverman, Jeffrey M. and Chornock, Ryan and Challis, Peter and Yuan, Wenlong and Brown, Peter J. and Foley, Ryan J.},
  date = {2016-07-21},
  journaltitle = {The Astrophysical Journal},
  shortjournal = {ApJ},
  volume = {826},
  pages = {56},
  issn = {1538-4357},
  doi = {10.3847/0004-637X/826/1/56},
  url = {http://arxiv.org/abs/1604.01424},
  urldate = {2020-03-05},
  abstract = {We use the Wide Field Camera 3 (WFC3) on the Hubble Space Telescope (HST) to reduce the uncertainty in the local value of the Hubble constant (H\_0) from 3.3\% to 2.4\%. Improvements come from new, near-infrared observations of Cepheid variables in 11 new hosts of recent SNe\textasciitilde Ia, more than doubling the sample of SNe\textasciitilde Ia having a Cepheid-calibrated distance for a total of 19; these leverage the magnitude-z relation based on 300 SNe\textasciitilde Ia at z{$<$}0.15. All 19 hosts and the megamaser system NGC4258 were observed with WFC3, thus nullifying cross-instrument zeropoint errors. Other improvements include a 33\% reduction in the systematic uncertainty in the maser distance to NGC4258, more Cepheids and a more robust distance to the LMC from late-type DEBs, HST observations of Cepheids in M31, and new HST-based trigonometric parallaxes for Milky Way (MW) Cepheids. We consider four geometric distance calibrations of Cepheids: (i) megamasers in NGC4258, (ii) 8 DEBs in the LMC, (iii) 15 MW Cepheids with parallaxes, and (iv) 2 DEBs in M31. H\_0 from each is 72.25+/-2.51, 72.04+/-2.67, 76.18+/-2.37, and 74.50+/-3.27 km/sec/Mpc, respectively. Our best estimate of 73.24+/-1.74 km/sec/Mpc combines the anchors NGC4258, MW, and LMC, and includes systematic errors for a final uncertainty of 2.4\%. This value is 3.4 sigma higher than 66.93+/-0.62 km/sec/Mpc predicted by LambdaCDM with 3 neutrinos with mass 0.06 eV and the Planck data, but reduces to 2.1 sigma relative to the prediction of 69.3+/-0.7 km/sec/Mpc with the combination of WMAP+ACT+SPT+BAO, suggesting systematic uncertainties in CMB measurements may play a role in the tension. If we take the conflict between Planck and H\_0 at face value, one plausible explanation could involve an additional source of dark radiation in the early Universe in the range of Delta N\_eff=0.4-1. We anticipate significant improvements in H\_0 from upcoming parallax measurements.},
  archivePrefix = {arXiv},
  eprint = {1604.01424},
  eprinttype = {arxiv},
  file = {/home/jacopo/zotero/storage/XUD5VTXF/Riess et al. - 2016 - A 2.4% Determination of the Local Value of the Hub.pdf;/home/jacopo/zotero/storage/CYUL7JII/1604.html},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Cosmology and Nongalactic Astrophysics},
  note = {Comment: accepted ApJ, includes proof corrections and edits, 63 pages, 16 figures, 8 tables. Table 4 available electronically by ApJ Revised since v1 to include one new supernova/calibrator and updated Planck constraints},
  number = {1}
}

@article{tanimuraSearchWarmHot2019,
  title = {A {{Search}} for {{Warm}}/{{Hot Gas Filaments Between Pairs}} of {{SDSS Luminous Red Galaxies}}},
  author = {Tanimura, Hideki and Hinshaw, Gary and McCarthy, Ian G. and Van Waerbeke, Ludovic and Aghanim, Nabila and Ma, Yin-Zhe and Mead, Alexander and Hojjati, Alireza and Tröster, Tilman},
  date = {2019-02-11},
  journaltitle = {Monthly Notices of the Royal Astronomical Society},
  volume = {483},
  pages = {223--234},
  issn = {0035-8711, 1365-2966},
  doi = {10.1093/mnras/sty3118},
  url = {http://arxiv.org/abs/1709.05024},
  urldate = {2020-07-30},
  abstract = {We search the Planck data for a thermal Sunyaev-Zel'dovich (tSZ) signal due to gas filaments between pairs of Luminous Red Galaxies (LRG's) taken from the Sloan Digital Sky Survey Data Release 12 (SDSS/DR12). We identify \$\textbackslash sim\$260,000 LRG pairs in the DR12 catalog that lie within 6-10 \$h\^\{-1\} \textbackslash mathrm\{Mpc\}\$ of each other in tangential direction and within 6 \$h\^\{-1\} \textbackslash mathrm\{Mpc\}\$ in radial direction. We stack pairs by rotating and scaling the angular positions of each LRG so they lie on a common reference frame, then we subtract a circularly symmetric halo from each member of the pair to search for a residual signal between the pair members. We find a statistically significant (5.3\$\textbackslash sigma\$) signal between LRG pairs in the stacked data with a magnitude \$\textbackslash Delta y = (1.31 \textbackslash pm 0.25) \textbackslash times 10\^\{-8\}\$. The uncertainty is estimated from two Monte Carlo null tests which also establish the reliability of our analysis. Assuming a simple, isothermal, cylindrical filament model of electron over-density with a radial density profile proportional to \$r\_c/r\$ (as determined from simulations), where \$r\$ is the perpendicular distance from the cylinder axis and \$r\_c\$ is the core radius of the density profile, we constrain the product of over-density and filament temperature to be \$\textbackslash delta\_c \textbackslash times (T\_\{\textbackslash rm e\}/10\^7 \textbackslash, \{\textbackslash rm K\}) \textbackslash times (r\_c/0.5h\^\{-1\} \textbackslash, \{\textbackslash rm Mpc\}) = 2.7 \textbackslash pm 0.5\$. To our knowledge, this is the first detection of filamentary gas at over-densities typical of cosmological large-scale structure. We compare our result to the BAHAMAS suite of cosmological hydrodynamic simulations (McCarthy et al. 2017) and find a slightly lower, but marginally consistent Comptonization excess, \$\textbackslash Delta y = (0.84 \textbackslash pm 0.24) \textbackslash times 10\^\{-8\}\$.},
  archivePrefix = {arXiv},
  eprint = {1709.05024},
  eprinttype = {arxiv},
  file = {/home/jacopo/zotero/storage/G44GMXYP/Tanimura et al. - 2019 - A Search for WarmHot Gas Filaments Between Pairs .pdf;/home/jacopo/zotero/storage/PGF679KI/1709.html},
  keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
  note = {Comment: 10 pages, 9 figures, accepted for publication in MNRAS (v4 and v5 to include funding acknowledgements. No other changes.)},
  number = {1}
}

