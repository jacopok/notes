  eprint = {1709.10378},
  eprinttype = {arxiv},
  file = {/home/jacopo/zotero/storage/IYTRV8HW/de Graaff et al. - 2019 - Probing the missing baryons with the Sunyaev-Zel'd.pdf;/home/jacopo/zotero/storage/YCLP7KGZ/1709.html},
