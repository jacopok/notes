@online{pacilioRankingLoveNeutron2021,
  title = {Ranking the {{Love}} for the Neutron Star Equation of State: The Need for Third-Generation Detectors},
  shorttitle = {Ranking the {{Love}} for the Neutron Star Equation of State},
  author = {Pacilio, Costantino and Maselli, Andrea and Fasano, Margherita and Pani, Paolo},
  date = {2021-09-21},
  eprint = {2104.10035},
  eprinttype = {arxiv},
  primaryclass = {astro-ph, physics:gr-qc},
  url = {http://arxiv.org/abs/2104.10035},
  urldate = {2021-09-22},
  abstract = {Gravitational-wave measurements of the tidal deformability in neutron-star binary coalescences can be used to infer the still unknown equation of state (EoS) of dense matter above the nuclear saturation density. By employing a Bayesian-ranking test we quantify the ability of current and future gravitational-wave observations to discriminate among families of nuclear-physics based EoS which differ in particle content and ab-initio microscopic calculations. While the constraining power of GW170817 is limited, we show that even twenty coalescences detected by LIGO-Virgo at design sensitivity are not enough to discriminate between EoS with similar softness but distinct microphysics. However, just a single detection with a third-generation detector such as the Einstein Telescope or Cosmic Explorer will rule out several families of EoS with very strong statistical significance, and can discriminate among models which feature similar softness, hence constraining the properties of nuclear matter to unprecedented levels.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - High Energy Astrophysical Phenomena,General Relativity and Quantum Cosmology},
  file = {/home/jacopo/zotero/storage/2GVFHG4G/Pacilio et al_2021_Ranking the Love for the neutron star equation of state.pdf;/home/jacopo/zotero/storage/ATYXE458/2104.html}
}

